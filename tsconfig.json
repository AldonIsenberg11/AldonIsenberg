{
    "compilerOptions": {
      "module": "commonjs",
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "outDir": "dist",
      "rootDir": "./",
      "sourceMap": true,
      "strict": false,
      "target": "es2018",
      "incremental": true,
      "resolveJsonModule": true,
      "types": [
          "jest",
      ]
    },
    "compileOnSave": true,
    "exclude": ["node_modules", "dist"],
    "include": [
      "src",
      "node_modules/@types/jest",
    ],
  }
  